<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="notice">

  <select id="noticeNew" parameterType="String" resultType="int">
    select count(*) from notice where nickname_to = #{id} and isread = 0
 </select>
 
 <select id="noticeGet" parameterType="String" resultType="notice">
    select * from notice where nickname_to = #{id}
 </select>

</mapper>
